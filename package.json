{
  "name": "kpop-stack-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate:css": "npx tailwindcss -o ./app/styles/tailwind.css",
    "lint:check": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "ntl:ci": "netlify dev",
    "start": "cross-env NODE_ENV=production netlify dev",
    "types:check": "tsc -b"
  },
  "prettier": {},
  "dependencies": {
    "@netlify/functions": "1.3.0",
    "@remix-run/netlify": "1.8.0",
    "@remix-run/node": "1.8.0",
    "@remix-run/react": "1.8.0",
    "@supabase/supabase-js": "2.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-invariant": "1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "1.8.0",
    "@remix-run/eslint-config": "1.8.0",
    "@remix-run/serve": "1.8.0",
    "@types/eslint": "8.4.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "cross-env": "7.0.3",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.0",
    "prettier-plugin-tailwindcss": "0.2.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.3"
  },
  "engines": {
    "node": ">=18"
  }
}
